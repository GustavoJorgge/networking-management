generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model intencaoParticipar {
    id        String   @id @default(uuid())
    name      String
    email     String
    empresa   String
    motivo    String
    status    String
    createdAt DateTime @default(now())
    idMembro  String?
}

model membro {
    id        String   @id @default(uuid())
    name      String
    email     String   @unique
    empresa   String
    telefone  String
    cargo     String
    status    String
    createdAt DateTime @default(now())
}
